<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" styleClass="vbox_style" stylesheets="/Styles/Test.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.omgtu.HGG.Controllers.LeftBarPropsViewController">
   <children>
      <Label text="Имя" />
      <TextField fx:id="nameTextField">
         <VBox.margin>
            <Insets bottom="20.0" left="10.0" right="10.0" />
         </VBox.margin>
      </TextField>
      <Label text="Фамилия" />
      <TextField fx:id="lastNameTextField" prefHeight="25.0" prefWidth="180.0">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </TextField>
      <Label text="Длинна зубов">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="206.0">
         <children>
            <TextField fx:id="toothLenght1TextField">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <Label text="L1" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="206.0">
         <children>
            <TextField fx:id="toothLenght2TextField" />
            <Label text="L2" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="206.0">
         <children>
            <TextField fx:id="toothLenght3TextField" />
            <Label text="L3" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#createDiagramAction" prefWidth="180.0" text="Построить диаграмму">
         <VBox.margin>
            <Insets top="60.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
